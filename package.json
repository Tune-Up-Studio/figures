{
  "name": "@tune-up-studio/figures",
  "version": "0.0.4",
  "description": "style definitions for tuneup studio",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./css/*": "./dist/css/designtokens.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "compile": "npx tsc",
    "js2css": "tokens-to js2css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tune-Up-Studio/figures.git"
  },
  "keywords": [
    "style",
    "token",
    "definition",
    "design"
  ],
  "author": "HoJoon Eum",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Tune-Up-Studio/figures/issues"
  },
  "homepage": "https://github.com/Tune-Up-Studio/figures#readme",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "tokens-to": "^0.0.5",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
